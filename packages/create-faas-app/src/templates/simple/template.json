{
  "name": "simple",
  "arguments": [
    {
      "name": "provider",
      "type": "string",
      "in": ["tencentcloud"]
    },
    {
      "name": "region",
      "type": "string",
      "in": ["ap-beijing", "ap-shanghai", "ap-guangzhou", "ap-hongkong"]
    },
    {
      "name": "appId",
      "type": "string",
      "match": "^[0-9]+$"
    },
    {
      "name": "secretId",
      "type": "string",
      "match": "^[a-zA-Z0-9]+$"
    },
    {
      "name": "secretKey",
      "type": "string",
      "match": "^[a-zA-Z0-9]+$"
    }
  ],
  "files": [
    "faas.yaml",
    "package.json"
  ]
}
