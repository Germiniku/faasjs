{
  "name": "auth",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts .",
    "test": "jest --silent --maxWorkers=1"
  },
  "dependencies": {
    "@faasjs/eslint-config-recommended": "beta",
    "faasjs": "beta",
    "@faasjs/knex": "beta",
    "sqlite3": "*"
  },
  "eslintConfig": {
    "extends": [
      "@faasjs/recommended"
    ]
  },
  "eslintIgnore": [
    "tmp"
  ],
  "jest": {
    "verbose": false,
    "transform": {
      ".(jsx|tsx?)": "@swc/jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "testRegex": "/*\\.test\\.ts$",
    "modulePathIgnorePatterns": [
      "/lib/",
      "/tmp/"
    ],
    "setupFilesAfterEnv": [
      "@faasjs/test/lib/jest.setup.js",
      "./jest.setup.ts"
    ]
  }
}
